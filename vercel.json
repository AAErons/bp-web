{
  "version": 2,
  "builds": [
    {
      "src": "dist/api/**/*.js",
      "use": "@vercel/node@2.15.8"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist",
        "buildCommand": "npm run build"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/galleries",
      "dest": "/dist/api/galleries/index.js"
    },
    {
      "src": "/api/galleries/(.*)",
      "dest": "/dist/api/galleries/[id].js"
    },
    {
      "src": "/api/upload",
      "dest": "/dist/api/upload.js"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "NODE_VERSION": "18.x"
  }
} 